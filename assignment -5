      "source": [
        " string=input(\"Enter a string: \")\n",
        "frequency={}\n",
        "for i in string:\n",
        "    if i in frequency:\n",
        "        frequency[i]+=1\n",
        "    else:\n",
        "        frequency[i]=1\n",
        "print(\"Character frequency\",string,\"is:\",frequency)"
      ],
      "execution_count": 8,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
            "Enter a string: Telugu\n",
